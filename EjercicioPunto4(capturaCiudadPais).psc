Proceso capturaCiudadPais
	Definir ciudadCapital Como Caracter;
	Definir pais Como Caracter;
	
	Escribir 'Por favor ingresa una ciudad capital: ';
	Leer ciudadCapital;
	Escribir 'Por favor ingresa un pais: ';
	Leer pais;
	
	Escribir 'La ciudad de ', ciudadCapital,' es la capital de ', pais;
FinProceso

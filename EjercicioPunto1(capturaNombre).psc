Proceso capturaNombre
	Definir nombre Como Caracter;
	Definir apellido1 Como Caracter;
	Definir apellido2 Como Caracter;
	
	Escribir 'Ingresa nombre: ';
	Leer nombre;
	Escribir 'Ingresa apellido1: ';
	Leer apellido1;
	Escribir 'Ingresa apellido2: ';
	Leer apellido2;
	
	Limpiar Pantalla;
	
	Escribir 'Nombre completo: ', nombre,' ',apellido1,' ',apellido2;
FinProceso
